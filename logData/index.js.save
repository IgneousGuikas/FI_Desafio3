$(document).ready(function() {

  $("table").css("visibility", "visible");
  $("#processT").hide();
  $("#alarmsT").hide();
  $("#activitiesT").hide();

  $("select").change(function() {

    var temp = $(this).children("option:selected").val();

    $("#processT").hide();
    $("#alarmsT").hide();
    $("#activitiesT").hide();

    $("#" + temp + "T").show();

    $.ajax({
      method: "GET",
      url: "http://18.223.194.18/getData/" + temp}).done();

  });

});
