server{
	listen 80 default_server;
	root /var/www/html;

	index index.html index.nginx-debian.html;

	location / {
		try_files $uri $uri/ =404;
	}



	location =/getData/process {
		proxy_pass http://localhost:3000;
	}
	location =/getData/alarms {
		proxy_pass http://localhost:3000;
	}
	location =/getData/activities {
		proxy_pass http://localhost:3000;
	}



	location =/getMachines {
		proxy_pass http://localhost:3000;
	}
	location =/getMachines/ID {
		proxy_pass http://localhost:3000;
	}

	location =/updateMachines/add {
		proxy_pass http://localhost:3000;
	}
	location =/updateMachines/del {
		proxy_pass http://localhost:3000;
	}
}
